#include <stdio.h>
#include <stdlib.h>


int main()
{ int a[2][2][3] , i ,j ,k;
for(i=0;i<2;i++)
{
    for(j=0;j<2;j++)
    {
        for(k=0;k<3;k++)
        {
            printf("please neter the value of index[%d][%d][%d] \n",i,j ,k);
        scanf("%d",&a[i][j][k]);
        }
    }
}
printf("array of a[2][4] is: \n");
printf("{");
for(i=0;i<2;i++)
{
    for(j=0;j<2;j++)
    {
          for(k=0;k<3;k++)
        {
            printf("%d ," , a[i][j][k]);
        }

    }
}
printf("}");



    return 0;
}
