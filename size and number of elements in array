#include <stdio.h>
#include <stdlib.h>


int main()
{ int a[]={1,2,3,4,5,6,9,8,7,1,2,3,4,5,6,7,8,9,7,8,9,6,5,4,1,2,3,5};
 printf("size of elements in array %d \n",sizeof(a));
 printf("size of 1 element in array %d \n",sizeof(a[0]));
 printf("number of elements in array %d \n",sizeof(a)/sizeof(a[0]));
    return 0;
}
