#include <stdio.h>
#include <stdlib.h>


int main()
{ int a[5][5] , i ,j ,sr=0 , sc=0;
for(i=0;i<5;i++)
{
    for(j=0;j<5;j++)
    {
       printf("please enter value of [%d][%d] \n" , i , j);
       scanf("%d",&a[i][j]);
    }

}

for(i=0;i<5;i++)
{
    for(j=0;j<5;j++)
    {
       sr+=a[i][j];

    }
    printf(" sr %d is %d \n",i,sr);
       sr=0;


}
for(j=0;j<5;j++)
{
    for(i=0;i<5;i++)
    {
       sc+=a[i][j];

    }
    printf(" sc %d is %d \n",j,sc);
       sc=0;


}

    return 0;
}
